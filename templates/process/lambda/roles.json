{"LambdaRole" : {
    "Type"  : "AWS::IAM::Role",
    "Properties" : {
        "AssumeRolePolicyDocument" : {
            "Version": "2012-10-17",
            "Statement": [{
                "Effect": "Allow",
                "Principal": {
                    "Service": "lambda.amazonaws.com"
                },
                "Action": "sts:AssumeRole"
            }]       
        },
        "Path" : "/",
        "ManagedPolicyArns":[
            "arn:aws:iam::aws:policy/service-role/AWSLambdaBasicExecutionRole",
            {"Ref":"KinesisPut"},
            {"Ref":"BucketPolicy"},
            {"Ref":"BucketPolicy2"}
        ]
    }
}}
